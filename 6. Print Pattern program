public class PatternProgram {
    public static void main(String[] args) {
        int n = 5; // number of rows in the pattern
        int num = 5; 
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(num);
            }
            num--;
            for (int k = i; k < n; k++) {
                System.out.print("5");
            }
            System.out.println();
        }
    }
}
